try:
 a=int(input("Enter the number:"))
 r=0
 if(a>0):
        while(a>0):
            d=a%10
            r=r*10+d
            a=a//10
        print("Mirror image:",r)
 
 elif r==0:
         b=str(a)
         r=b[::-1]
         print("Mirror image:",r)
 else:
     print("in")
except ValueError:
    print("INVALID INPUT")
